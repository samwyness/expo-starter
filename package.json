{
  "name": "expo-starter",
  "description": "An Expo starter template for creating cross platform applications",
  "version": "1.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "postinstall": "npx patch-package",
    "gen:tokens": "npx skits-tokens sync",
    "prebuild": "expo prebuild --clean",
    "start": "expo start -c --dev-client",
    "start:prod": "expo start --dev-client --no-dev --minify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "test:ci": "NODE_ENV=test jest --ci --forceExit",
    "test:coverage": "NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "@clerk/backend": "^2.27.0",
    "@clerk/clerk-expo": "^2.19.11",
    "@convex-dev/react-query": "^0.1.0",
    "@dev-plugins/react-query": "^0.4.0",
    "@material-symbols/svg-400": "^0.40.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.4.2",
    "@skits/figma-tokens": "^2.2.0",
    "@tanstack/react-query": "^5.87.4",
    "convex": "^1.26.2",
    "expo": "~54.0.27",
    "expo-auth-session": "~7.0.10",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.11",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.10",
    "expo-local-authentication": "~17.0.8",
    "expo-router": "~6.0.17",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-unistyles": "^3.0.19",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "svix": "^1.73.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.26.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.1",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.13",
    "patch-package": "^8.0.0",
    "prettier": "^3.5.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  }
}
