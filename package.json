{
  "name": "expo-starter",
  "description": "An Expo starter template for creating cross platform applications",
  "version": "1.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "postinstall": "npx patch-package",
    "gen:tokens": "npx skits-tokens sync",
    "prebuild": "expo prebuild --clean",
    "start": "expo start --dev-client",
    "start:prod": "expo start --dev-client --no-dev --minify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "test:ci": "NODE_ENV=test jest --ci --forceExit",
    "test:coverage": "NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.14.21",
    "@material-symbols/svg-400": "^0.34.1",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/elements": "^2.3.7",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.4.2",
    "@skits/figma-tokens": "^2.2.0",
    "expo": "53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-linking": "~7.1.7",
    "expo-local-authentication": "~16.0.5",
    "expo-router": "~5.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-edge-to-edge": "^1.6.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-nitro-modules": "^0.27.6",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-unistyles": "^3.0.9",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "zod": "^3.25.67",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-af1b7da-20250417",
    "eslint": "^9.26.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.1",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
    "patch-package": "^8.0.0",
    "prettier": "^3.5.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  }
}
